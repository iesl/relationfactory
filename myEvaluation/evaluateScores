predictionFile=$1
lhsFile=$2
exampleDir=$3
thresh=$4
outReport=$5

tmpPrediction=`mktemp`
paste  $lhsFile $predictionFile > $tmpPrediction.1
perl /iesl/canvas/belanger/relationfactory/myEvaluation/filterByThresh.pl $tmpPrediction.1 $thresh> $tmpPrediction


echo sh /iesl/canvas/belanger/relationfactory/myEvaluation/evaluate $tmpPrediction $exampleDir $outReport  
sh /iesl/canvas/belanger/relationfactory/myEvaluation/evaluate $tmpPrediction $exampleDir $outReport  

rm $tmpPrediction*